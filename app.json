{
  "expo": {
    "name": "Steya",
    "slug": "Steya",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/applogo.png",
    "scheme": "steya",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "runtimeVersion": "1.0.0",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/applogo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "CAMERA",
        "RECORD_AUDIO",
        "READ_MEDIA_IMAGES",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.ameen007.Steya",
      "googleServicesFile": "./google-services.json"   
    },
    "androidNavigationBar": {
      "barStyle": "light-content",
      "backgroundColor": "#ffffff"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/applogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#7A5AF8"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Steya to access your location for better service.",
          "locationWhenInUsePermission": "Allow Steya to use your location"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Steya to access your camera",
          "microphonePermission": "Allow Steya to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/applogo.png",
          "color": "#7A5AF8",
          "defaultChannel": "chat-messages",
          "enableBackgroundRemoteNotifications": false
        }
      ],
      "expo-font",
      "expo-web-browser",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps._some_id_here_"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c4e857a3-fd54-40ba-bc0a-0a1bce49b3b0"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/c4e857a3-fd54-40ba-bc0a-0a1bce49b3b0"
    }
  }
}
